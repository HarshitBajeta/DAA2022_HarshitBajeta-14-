#include <stdio.h>
int main()
{
    int n,n1,c=0;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("enter the key to be search : ");
    scanf("%d",&n1);
     for(int i=0;i<n;i++){
         if(n1==a[i]){
             c++;
             printf("the key is at index : %d",i);
             printf("no. of comparisions : %d",++i);
         }
     }
     if(c==0)
     printf("-1");
     

    return 0;
}

